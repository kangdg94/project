class Main {   
    static void main() {
    		int i;
				i = 0;
				while(i<10){
					i = i+1;
					if(i>5){
						continue;
					}
					Print(i,"\n");
				}
				Print(i,"\n\n");

				i = 0;
				for(i = 0 ;i<34; i=i+2){
					if(i>15){
						continue;
					}
					Print(i,"\n");
				}
				Print(i,"\n\n");
							
				i = 0;
				repeat{
					i = i+1;
					if(i>13){
						continue;
					}
					Print(i,"\n");
				}until(i>=30);
				Print(i,"\n\n");
    }
}